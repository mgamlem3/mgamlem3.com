{
	"name": "mgamlem3.com",
	"version": "1.0.1",
	"main": "index.js",
	"repository": "github.com/mgamlem3/mgamlem3.com",
	"author": "Michael Gamlem III",
	"license": "MIT",
	"private": false,
	"scripts": {
		"dev": "webpack serve",
		"prebuild": "yarn prettier-check && yarn eslint && yarn stylelint-check",
		"build": "rm -rf dist && webpack --mode production --config webpack.prod.config.js && webpack --mode production --config webpack.server.config.js",
		"test": "yarn prettier-check; yarn build",
		"start": "node ./dist/server.*.wp.js",
		"prettier": "prettier --config ./.prettierrc --ignore-path  ./.prettierignore --write \"**/*.ts*\" \"**/*.js\" \"**/*.scss\" \"*.json\" \"*.md\"",
		"prettier-check": "prettier --list-different \"{*.js, *.md,./{src}/**/*.ts?(x)}\"",
		"lint": "yarn prettier; yarn eslint; yarn stylelint",
		"eslint": "eslint '*/**/*.{js,ts,tsx}'",
		"stylelint": "stylelint --config ./.stylelintrc --fix \"**/*.scss\" --color --cache",
		"stylelint-check": "stylelint --config ./.stylelintrc \"**/*.scss\" --color --cache"
	},
	"browsersList": [
		"last 2 versions",
		"not dead",
		"> 0.2%"
	],
	"dependencies": {
		"express": "^4.17.1",
		"fs": "^0.0.2",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-router-dom": "^5.1.2"
	},
	"devDependencies": {
		"@babel/core": "^7.13.8",
		"@babel/preset-env": "^7.13.8",
		"@babel/preset-react": "^7.12.13",
		"@babel/preset-typescript": "^7.13.0",
		"@types/express": "^4.17.11",
		"@types/node": "^14.14.31",
		"@types/react": "^17.0.2",
		"@types/react-dom": "^17.0.1",
		"@types/react-router": "^5.1.11",
		"@types/react-router-dom": "^5.1.7",
		"@typescript-eslint/eslint-plugin": "^4.15.2",
		"@typescript-eslint/parser": "^4.15.2",
		"@webpack-cli/serve": "^1.3.0",
		"babel-loader": "^8.2.2",
		"compression-webpack-plugin": "^7.1.2",
		"css-loader": "^5.1.0",
		"css-minimizer-webpack-plugin": "^1.2.0",
		"eslint": "^7.21.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-prettier": "^3.3.1",
		"eslint-plugin-react": "^7.22.0",
		"file-loader": "^6.0.0",
		"html-webpack-plugin": "^5.2.0",
		"mini-css-extract-plugin": "^1.3.9",
		"path": "^0.12.7",
		"prettier": "^2.2.1",
		"react-is": "^17.0.1",
		"sass": "^1.32.8",
		"sass-loader": "^11.0.1",
		"style-loader": "^2.0.0",
		"stylelint": "^13.11.0",
		"stylelint-config-prettier": "^8.0.2",
		"stylelint-prettier": "^1.2.0",
		"terser-webpack-plugin": "^5.1.1",
		"typescript": "^4.2.2",
		"webpack": "^5.24.2",
		"webpack-cli": "^4.5.0",
		"webpack-dev-server": "^3.11.2",
		"webpack-node-externals": "^2.5.2"
	}
}
